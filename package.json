{
  "name": "@gpe-sistemas/tripero-node",
  "version": "0.5.0",
  "description": "Node.js/TypeScript SDK for Tripero GPS trip detection service",
  "author": "GPE Sistemas",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.js"
    },
    "./nestjs": {
      "types": "./dist/nestjs/index.d.ts",
      "require": "./dist/nestjs/index.js",
      "import": "./dist/nestjs/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "lint": "eslint 'src/**/*.ts'",
    "format": "prettier --write 'src/**/*.ts'"
  },
  "keywords": [
    "tripero",
    "gps",
    "trip-detection",
    "stop-detection",
    "fleet-management",
    "tracking",
    "nestjs",
    "typescript",
    "redis",
    "telematics"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GPE-Sistemas/tripero-node.git"
  },
  "bugs": {
    "url": "https://github.com/GPE-Sistemas/tripero-node/issues"
  },
  "homepage": "https://github.com/GPE-Sistemas/tripero-node#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "peerDependencies": {
    "@nestjs/common": ">=10.0.0",
    "@nestjs/core": ">=10.0.0"
  },
  "peerDependenciesMeta": {
    "@nestjs/common": {
      "optional": true
    },
    "@nestjs/core": {
      "optional": true
    }
  },
  "dependencies": {
    "ioredis": "^5.4.1"
  },
  "devDependencies": {
    "@nestjs/common": "^11.0.1",
    "@nestjs/core": "^11.0.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.1",
    "@typescript-eslint/eslint-plugin": "^8.16.0",
    "@typescript-eslint/parser": "^8.16.0",
    "eslint": "^9.15.0",
    "jest": "^29.7.0",
    "prettier": "^3.4.1",
    "reflect-metadata": "^0.2.2",
    "ts-jest": "^29.2.5",
    "typescript": "^5.7.2"
  }
}
